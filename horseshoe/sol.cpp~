#include <bits/stdc++.h>

#define maxn 1000007
#define pp push_back
#define bb pop_back

using namespace std;

int main(){
	int a,b,c,d;
	scanf("%d%d%d%d",&a,&b,&c,&d);
	int ans = 0;
	if(a==b)ans++;
	if(a==c)ans++;
	if(a==d)ans++;
	if(b==c)ans++;
	if(b==d)ans++;
	if(c==d)ans++;
	printf("%d\n",ans);
	return 0;
}
